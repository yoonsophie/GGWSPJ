<body>
	<div>
	<div class="graphBox">
		<canvas id="myChart" width="384" height="210"></canvas>
	</div>
	<!-- 푸시 -->
	<div class="graphBox">
		<canvas id="myChart" width="384" height="210"></canvas>
	</div>
	</div>

<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<script>
  const ctx = document.getElementById('myChart');

  new Chart(ctx, {
    type: 'bar',
    data: {
      labels: ['Red', 'Blue', 'Yellow', 'Green', 'Purple', 'Orange'],
      datasets: [{
        label: '# of Votes',
        data: [12, 19, 3, 5, 2, 3],
        borderWidth: 1
      }]
    },
    options: {
      scales: {
        y: {
          beginAtZero: true
        }
      }
    }
  });
</script>

				
</body>
</html>